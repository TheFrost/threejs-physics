{
  "name": "threejs-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "TheFrost <frost084@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf .cache/ && npx parcel src/index.html --no-source-maps -d public/ --open",
    "build": "rm -rf build/ && npx parcel build src/index.html --no-source-maps -d build/ && cp -R public/resources build/resources"
  },
  "dependencies": {
    "ammo.js": "^0.0.10",
    "stats.js": "^0.17.0",
    "three": "^0.103.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "json5": "^2.1.0",
    "parcel-bundler": "^1.10.1",
    "parcel-plugin-glsl": "^0.1.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
